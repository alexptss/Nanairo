import QtQuick.Controls @qt_quick_controls_version@
import "../Styles"

Button {
  id: button_item

  property color backgroundColor

  width: @default_image_button_size@
  height: @default_image_button_size@

  text: ""
  style: NImageButtonStyle {
    backgroundColor: button_item.backgroundColor
  }
}
