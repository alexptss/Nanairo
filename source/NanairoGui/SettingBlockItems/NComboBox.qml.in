import QtQuick @qt_quick_version@
import QtQuick.Controls @qt_quick_controls_version@
import "../Styles"

ComboBox {
  id: combobox_item

  property color backgroundColor

  anchors.horizontalCenter: parent.horizontalCenter
  width: @default_combobox_width@
  height: @default_combobox_height@

  model: []
  style: NComboBoxStyle {
    fontPixelSize: 10
    backgroundColor: combobox_item.backgroundColor
  }
}
