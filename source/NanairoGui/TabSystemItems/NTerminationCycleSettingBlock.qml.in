import QtQuick @qt_quick_version@
import QtQuick.Controls @qt_quick_controls_version@
import "../SettingBlockItems"

NSettingBlock {
  id: termination_cycle_setting_block

  labelText: qsTr("TerminationCycle")

  NIntSpinBox {
    id: termination_cycle_spinbox

    y: termination_cycle_setting_block.labelHeight + @item_offset@

    maximumValue: @integer32_max@
    minimumValue: 0
  }

  function getSceneData() {
    var sceneData = {};
    sceneData["@terminationCycle@"] = termination_cycle_spinbox.value;
    return sceneData;
  }

  function setSceneData(sceneData) {
    termination_cycle_spinbox.value = sceneData["@terminationCycle@"];
  }
}
