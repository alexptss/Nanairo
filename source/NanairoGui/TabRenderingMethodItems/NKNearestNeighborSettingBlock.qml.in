import QtQuick @qt_quick_version@
import QtQuick.Controls @qt_quick_controls_version@
import "../SettingBlockItems"

NSettingBlock {
  id: k_nearest_neighbor_setting_block

  labelText: qsTr("KNearestNeighbor")

  NIntSpinBox {
    id: k_nearest_neighbor_spinbox

    y: k_nearest_neighbor_setting_block.labelHeight + @item_offset@

    maximumValue: @integer32_max@
    minimumValue: 1
  }

  function initialize() {
    k_nearest_neighbor_spinbox.value = 8;
  }

  function getSceneData() {
    var sceneData = {};
    sceneData["@kNearestNeighbor@"] = k_nearest_neighbor_spinbox.value;
    return sceneData;
  }

  function setSceneData(sceneData) {
    k_nearest_neighbor_spinbox.value = sceneData["@kNearestNeighbor@"];
  }
}
