import QtQuick @qt_quick_version@
import QtQuick.Controls @qt_quick_controls_version@
import "../SettingBlockItems"

NSettingBlock {
  id: gamma_setting_block
  
  labelText: qsTr("GammaCorrection")

  NSpinBox {
    id: gamma_spinbox 

    y: gamma_setting_block.labelHeight + @item_offset@

    maximumValue: 99.9999999
    minimumValue:  0.0000001
  }

  function getSceneData() {
    var sceneData = {};
    sceneData["@gamma@"] = gamma_spinbox.value;
    return sceneData;
  }

  function setSceneData(sceneData) {
    gamma_spinbox.value = sceneData["@gamma@"];
  }
}
